<html>
<head>
  <%= render '/shared/head' %>
</head>
<%= render 'shared/top' %>
<form action="<%= companies_url %>" method="post">
	<label for="user_email">Your email address:</label>
	<input type="text" name="user[email]" id="user_email">
	<br>
	<label for="user_password">Your password:</label>
	<input type="password" name="user[password]" id="user_password">
	<br>
	<label for="user_name">Your full name:</label>
	<input type="text" name="user[name]" id="user_name">
	<br>
	<label for="company_name">Company or Organization:</label>
	<input type="text" name="company[name]" id="company_name">
	<br>
	<input type="Submit" value="Start my free trial">
</form>
</html>