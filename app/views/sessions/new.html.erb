<div class="new-session-wrapper">
	<div id="website-description">Rampup Ready is an UrbanDictionary clone for companies to create employee-sourced internal-only dictionaries to train employees faster.</div>
  <h2>Log In</h2>
  <form action="<%= session_url %>" method="post">
    <label for="user_email">Email:</label>
    <input type="text" id="user_email" name="user[email]">
    <label for="user_password">Password:</label>
    <input type="password" id="user_password" name="user[password]">
    <input type="submit" value="Sign In">
  </form>
  <%= link_to "Forgot Password?", forgot_password_url %>
	<form action="<%= session_url %>" method="post">
		<input type="hidden" name="user[email]" value="demouser">
		<input type="hidden" name="user[password]" value="password">
		<input type="submit" value="Demo Login">
	</form>
</div>