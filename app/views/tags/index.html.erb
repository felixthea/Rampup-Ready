<div class="tags-wrapper">
  <h2>Tags</h2>
  <ul>
    <% @tags.each do |tag| %>
      <li>
        <%= link_to tag.name, tag_url(tag.id) %>
        <%= button_to "Delete", tag_url(tag.id), method: :delete, class: "submit" %>
      </li>
    <% end %>
  </ul>
  <%= link_to "Create Tag", new_tag_url %>
</div>